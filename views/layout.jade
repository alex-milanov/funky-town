html
	head
		title FunkyTOWN Ibiza
		link(rel="stylesheet",type="text/css",href="/lib/bootstrap/dist/css/bootstrap.css")
		link(rel="stylesheet",type="text/css",href="/lib/fontawesome/css/font-awesome.css")
		link(rel="stylesheet",type="text/css",href="/css/style.css")
	body

		nav.navbar.navbar-inverse
			.container-fluid
				.navbar-header
					button.navbar-toggle.collapsed(data-toggle="collapse",data-target="#my-menu")
						i.fa.fa-bars
					a(href="/").navbar-brand FunkyTOWN Ibiza

				.collapse.navbar-collapse#my-menu
					ul.nav.navbar-nav.navbar-left
						li: a(href="/events") Events
						li: a(href="/artists") Artists

		block content



		//- dependencies
		script(src="lib/jquery/dist/jquery.js")
		script(src="lib/q/q.js")
		script(src="lib/bootstrap/dist/js/bootstrap.js")
		script(src="lib/jade/jade.js")
		//- tools
		script(src="js/tools/resource.js")
		script(src="js/tools/helpers.js")
		script(src="js/tools/router.js")

		block scripts
		